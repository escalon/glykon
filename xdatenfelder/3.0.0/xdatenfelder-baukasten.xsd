      <xs:extension base="xoev-lc:String.Latin">
        <xs:attribute name="maxLength" type="xs:positiveInteger" use="optional">
          <xs:annotation>
            <xs:documentation>Maximale Länge der einzugebenden Daten.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="maxValue" type="xs:token" use="optional">
          <xs:annotation>
            <xs:documentation>Maximaler Wert der angegeben werden muss. Dies ist nur bei geeigneten Datentypen wie Zahlen oder Datumsangaben möglich. Der angegebene Wert ist als Eingabe gültig (inklusiv).</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="minLength" type="xs:nonNegativeInteger" use="optional">
          <xs:annotation>
            <xs:documentation>Minimale Länge der einzugebenden Daten.</xs:documentation>

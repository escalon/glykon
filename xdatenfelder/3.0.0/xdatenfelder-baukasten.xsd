      <xs:element name="typ" type="xdf:Code.Regeltyp">
        <xs:annotation>
          <xs:documentation>Typ der Regel, z.B. Validierungsregel.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="param" minOccurs="0" maxOccurs="unbounded" type="xdf:RegelParameter">
        <xs:annotation>
          <xs:documentation>Eingabewert (Parameter) für das Script.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ziel" minOccurs="0" maxOccurs="unbounded" type="xdf:RegelZiel">
        <xs:annotation>
          <xs:documentation>Element, auf das die Regel wirkt.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="skript" minOccurs="0" type="xs:string">
        <xs:annotation>
          <xs:documentation>Das Skript, welches die technische Repräsentation einer zu erfüllenden Bedingung wiedergibt.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="fehler" minOccurs="0" maxOccurs="unbounded" type="xdf:Fehlermeldung">
